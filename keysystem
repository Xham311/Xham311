label.TextSize = 14.000

ks.Name = "ks"
ks.Parent = LoginUi
ks.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
ks.BorderColor3 = Color3.fromRGB(15, 15, 15)
ks.BorderSizePixel = 0
